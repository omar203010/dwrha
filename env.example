# Django Settings
# نسخ هذا الملف إلى .env واملأ القيم الصحيحة
# NEVER commit .env file to version control!

DEBUG=True
SECRET_KEY=your-secret-key-here-change-this-in-production-generate-a-secure-random-key
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Settings (SQLite for development, PostgreSQL for production)
# For SQLite (development), leave these commented:
# DB_NAME=
# DB_USER=
# DB_PASSWORD=
# DB_HOST=
# DB_PORT=

# For PostgreSQL (production), uncomment and fill:
# DB_NAME=dawerha_db
# DB_USER=dawerha_user
# DB_PASSWORD=your-secure-db-password-here
# DB_HOST=localhost
# DB_PORT=5432

# Email Settings (Optional)
# EMAIL_HOST=smtp.gmail.com
# EMAIL_PORT=587
# EMAIL_USE_TLS=True
# EMAIL_HOST_USER=your-email@gmail.com
# EMAIL_HOST_PASSWORD=your-app-password-here

